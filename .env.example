# Dremio Configuration
DREMIO_USERNAME=your_username
DREMIO_PASSWORD=your_password
DREMIO_SERVER=https://dremio-pre-01.eea.europa.eu:9047
DREMIO_SERVER_AUTH=https://dremio-pre-01.eea.europa.eu:9047
DREMIO_SSL=false
DREMIO_LIMIT=300000 
DREMIO_TIMEOUT=60000

# API Mode Configuration
# Options: "dremio" (direct) or "middleware" (EEA REST API)
API_MODE=middleware

# EEA Middleware API Configuration (used when API_MODE=middleware)
EEA_MIDDLEWARE_BASE_URL=https://restapi.eea.europa.eu
# For local development:
# EEA_MIDDLEWARE_BASE_URL=https://127.0.0.1:7181
